#casa, senha = "minha", "ola"
#print(casa, senha)

#SEGUNDA ATRIBUIÇÃO: DÁ ERRO PORQUE TEM DUAS ATRIBUIÇÕES PRA UMA VARIÁVEL DECLARADA;
casa, senha = "minha"
print(casa, senha)

#TERCEIRA ATRIBUIÇÃO: DEU CERTO, UMA ATRIBUIÇÃO PARA UMA VARIÁVEL;
#casa = "minha"
#print(casa)
